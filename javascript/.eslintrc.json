{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {
            "globalReturn": true,
            "impliedStrict": true,
            "jsx": false
        }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true,
        "jasmine": true
    },
    "globals": {
        "GlobalA": false,
        "MyGlobal": true
    },
    "plugins": [ ],
    "rules": {
        "indent": [ "error", 4 ],
        "linebreak-style": [ 2, "unix" ],
        "quotes": [ 2, "single", "avoid-escape" ],
        "semi": [ 2, "always" ],
        "comma-dangle": [ 2, "never" ],
        "no-cond-assign": [ 2, "always" ],
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [ 2, "functions" ],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [ 2, "both" ],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0,
        "valid-typeof": 2,
        "accessor-pairs": [
            2,
            {
                "getWithoutSet": false,
                "setWithoutGet": true
            }
        ],
        "array-callback-return": 1,
        "block-scoped-var": 0,
        "complexity": [ 2, 20 ],
        "consistent-return": 1,
        "curly": [ 2, "all" ],
        "default-case": 2,
        "dot-location": [ 2, "property" ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true,
                "allowPattern": ""
            }
        ],
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 0,
        "no-empty-function": [
            2,
            { "allow": [] }
        ],
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": [
            2,
            { "exceptions": [] }
        ],
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [
            2,
            {
                "boolean": true,
                "number": true,
                "string": true,
                "allow": []
            }
        ],
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 0,
        "no-iterator": 2,
        "no-labels": [
            2,
            {
                "allowLoop": false,
                "allowSwitch": false
            }
        ],
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": [
            0,
            {
                "ignore": [],
                "ignoreArrayIndexes": false,
                "enforceConst": false,
                "detectObjects": false
            }
        ],
        "no-multi-spaces": [
            1,
            {
                "exceptions": {
                    "VariableDeclarator": true,
                    "ImportDeclaration": true
                }
            }
        ],
        "no-multi-str": 2,
        "no-native-reassign": [
            1,
            { "exceptions": [] }
        ],
        "no-new": 1,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": [
            0,
            { "props": false }
        ],
        "no-process-env": 0,
        "no-proto": 2,
        "no-redeclare": [
            2,
            { "builtinGlobals": true }
        ],
        "no-return-assign": [ 2, "always" ],
        "no-script-url": 2,
        "no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": [
            2,
            {
                "allowShortCircuit": true,
                "allowTernary": false
            }
        ],
        "no-unused-labels": 2,
        "no-useless-call": 2,
        "no-useless-concat": 0,
        "no-void": 2,
        "no-warning-comments": [
            1,
            {
                "terms": [ "fixme" ],
                "location": "start"
            }
        ],
        "no-with": 2,
        "radix": [ 2, "always" ],
        "vars-on-top": 0,
        "wrap-iife": [ 2, "outside" ],
        "yoda": [
            2,
            "never",
            {
                "exceptRange": true,
                "onlyEquality": false
            }
        ],
        "strict": [ 0, "safe" ],
        "init-declarations": [ 0, "always" ],
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow": [
            1,
            {
                "builtinGlobals": true,
                "hoist": "functions",
                "allow": []
            }
        ],
        "no-shadow-restricted-names": 2,
        "no-undef": [
            0,
            { "typeof": false }
        ],
        "no-undef-init": 0,
        "no-undefined": 0,
        "no-unused-vars": [
            1,
            {
                "vars": "local",
                "args": "none",
                "varsIgnorePattern": "",
                "argsIgnorePattern": ""
            }
        ],
        "no-use-before-define": [
            0,
            {
                "functions": false,
                "classes": true
            }
        ],
        "callback-return": [
            2,
            [
                "cb",
                "callback",
                "next"
            ]
        ],
        "global-require": 2,
        "handle-callback-err": [ 2, "^(err|error)$" ],
        "no-mixed-requires": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-restricted-imports": 0,
        "no-restricted-modules": 0,
        "no-sync": 2,
        "array-bracket-spacing": [
            0,
            "always",
            {
                "singleValue": true,
                "objectsInArrays": true,
                "arraysInArrays": true
            }
        ],
        "block-spacing": [
            0,
            "always"
        ],
        "brace-style": [
            1,
            "1tbs",
            { "allowSingleLine": true }
        ],
        "camelcase": [
            1,
            { "properties": "always" }
        ],
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [ 2, "last" ],
        "computed-property-spacing": [
            0,
            "always"
        ],
        "consistent-this": [
            2,
            "SELF",
            "template",
            "ctx"
        ],
        "eol-last": [ 2, "unix" ],
        "func-names": 0,
        "func-style": [
            2,
            "expression",
            { "allowArrowFunctions": true }
        ],
        "id-blacklist": [
            0,
            "data"
        ],
        "id-length": [
            0,
            {
                "min": 2,
                "max": 30,
                "properties": "never",
                "exceptions": [ "a", "b", "c", "x", "y" ]
            }
        ],
        "id-match": [
            0,
            "^[a-z]+([A-Z][a-z]+)*$",
            { "properties": false }
        ],
        "jsx-quotes": [ 2, "prefer-double" ],
        "key-spacing": [
            2,
            {
                "beforeColon": false,
                "afterColon": true,
                "mode": "minimum"
            }
        ],
        "keyword-spacing": 2,
        "lines-around-comment": [
            0,
            {
                "beforeBlockComment": false,
                "afterBlockComment": false,
                "beforeLineComment": true,
                "afterLineComment": true,
                "allowBlockStart": false,
                "allowBlockEnd": false,
                "allowObjectStart": true,
                "allowObjectEnd": true,
                "allowArrayStart": true,
                "allowArrayEnd": true
            }
        ],
        "max-depth": [ 2, 6 ],
        "max-len": [
            2,
            {
                "code": 100,
                "tabWidth": 4,
                "ignoreComments": false,
                "ignoreTrailingComments": false,
                "ignoreUrls": true
            }
        ],
        "max-nested-callbacks": [ 2, 6 ],
        "max-params": [ 2, 10 ],
        "max-statements": [
            0,
            10,
            { "ignoreTopLevelFunctions": true }
        ],
        "new-cap": [
            2,
            {
                "newIsCap": true,
                "capIsNew": false,
                "newIsCapExceptions": [],
                "capIsNewExceptions": [],
                "properties": false
            }
        ],
        "new-parens": 2,
        "newline-after-var": [ 0, "always" ],
        "newline-per-chained-call": [
            0,
            { "ignoreChainWithDepth": 3 }
        ],
        "no-array-constructor": 2,
        "no-bitwise": [
            0,
            {
                "int32Hint": false,
                "allow": [
                    "~", "^",
                    "<<", ">>>", ">>",
                    "|=", "&=", "^=",
                    "<<=", ">>=", ">>>="
                ]
            }
        ],
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [
            0,
            {
                "max": 7,
                "maxBOF": 2,
                "maxEOF": 1
            }
        ],
        "no-negated-condition": 0,
        "no-nested-ternary": 0,
        "no-new-object": 0,
        "no-plusplus": [
            0,
            { "allowForLoopAfterthoughts": true }
        ],
        "no-restricted-syntax": [
            2,
            "ClassDeclaration",
            "ClassExpression"
        ],
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": [
            2,
            { "defaultAssignment": false }
        ],
        "no-whitespace-before-property": 2,
        "object-curly-spacing": [
            0,
            "always",
            {
                "objectsInObjects": true,
                "arraysInObjects": true
            }
        ],
        "one-var": [
            0,
            {
                "var": "never",
                "let": "never",
                "const": "never"
            }
        ],
        "one-var-declaration-per-line": [
            0,
            "initializations"
        ],
        "operator-assignment": [ 0, "always" ],
        "operator-linebreak": [
            2,
            "before",
            {
                "overrides": { "?": "after" }
            }
        ],
        "padded-blocks": [ 0, "always" ],
        "quote-props": [
            2,
            "as-needed",
            {
                "keywords": true,
                "unnecessary": false,
                "numbers": true
            }
        ],
        "require-jsdoc": [
            0,
            {
                "require": {
                    "FunctionDeclaration": true,
                    "ClassDeclaration": false,
                    "MethodDefinition": false
                }
            }
        ],
        "semi-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "sort-imports": [
            0,
            {
                "ignoreCase": true,
                "ignoreMemberSort": true,
                "memberSyntaxSortOrder": [ "none", "all", "multiple", "single" ]
            }
        ],
        "sort-vars": [
            0,
            { "ignoreCase": true }
        ],
        "space-before-blocks": 2,
        "space-before-function-paren": [
            2,
            "always"
        ],
        "space-in-parens": [
            0,
            "always",
            {
                "exceptions": [ "empty" ]
            }
        ],
        "space-infix-ops": [
            2,
            { "int32Hint": false }
        ],
        "space-unary-ops": [
            2,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "spaced-comment": [
            0,
            "always",
            {
                "line": {
                    "markers": [],
                    "exceptions": [ "-", "+", "#", "*" ]
                }
            }
        ],
        "wrap-regex": 2,
        "arrow-parens": [ 2, "always" ],
        "arrow-spacing": [
            0,
            {
                "before": false,
                "after": false
            }
        ],
        "constructor-super": 2,
        "generator-star-spacing": [
            0,
            {
                "before": false,
                "after": true
            }
        ],
        "no-class-assign": 2,
        "no-confusing-arrow": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-new-symbol": 2,
        "no-this-before-super": 2,
        "no-useless-constructor": 0,
        "no-var": 0,
        "object-shorthand": [
            0,
            "methods",
            { "ignoreConstructors": false }
        ],
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-reflect": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 2,
        "prefer-template": 1,
        "require-yield": 2,
        "template-curly-spacing": [ 0, "always" ],
        "yield-star-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ]
    }
}
